anchors:       @a          @b[2]       @c[2]     @d[3]        @e     @f     @g     @h       @j[3]    @i[2]        @l
time:    rlx   @a  t1       @b2 t2      @c2 t3    @d2  T1a    @e T1b @f T12 @g T12 @h  d    @j2    d @i1-i2  aq   @l
rf H:        90@a                                                        180@g                       acq@i2--------l   
rf 13C:                   90@b2       90@c2     90@d2      180@e   90@f                  180@j2    90@i1
pfg  z:                  g1@b1       g2@c1    g3@d1 g4@d3                              g5@j1 g6@j3     

[delays]
T1b: show_at=13C label=\frac{\Delta-\kappa t_1}{2}
T1a: show_at=13C label=\frac{\Delta+\kappa t_1}{2}
t2: show_at=13C label=\tau_2
t1: show_at=13C label=\tau_1
t3: show_at=13C label=\tau_3
T12: show_at=13C label=\frac{t_1}{2}
rlx: hide=true
d: label=\delta

[rf channels]
rfchan 13C: label=^{13}C nucleus=C
rfchan H: label=^1H nucleus=H

[pfg channels]
pfgchan z: label=Grad.\ z

[rf events]
90@f: phase=phi1 arrow=left 
acq: phase=phi2 type=fid

[phases]
phase phi1: label=\phi_1 table=0,2
phase phi2: label=\phi_2 table=1,3

[gradients]
g1: strength=70 label=g_1
g2: strength=-40 label=g_2
g3: strength=-20 label=g_3
g4: strength=-10 label=g_4
g5: strength=50,-30 label=g_5
g6: strength=-30,50 label=g_6
